class Solution{
public:
    int remove_duplicate(int a[],int n){
        // code here
        
        
    int j = 0;
       for(int i = 1; i < n; i++)
       { 
           
           if(a[i] == a[j])
           {
               continue;
               
           }
           else{
               
               j++;
               a[j] =a[i];
           }
       }
       return j+1;
   }
         
};
